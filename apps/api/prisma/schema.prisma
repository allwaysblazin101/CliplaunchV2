datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id        Int      @id @default(autoincrement())
  address   String   @unique
  createdAt DateTime @default(now())
}

model Video {
  id          Int      @id @default(autoincrement())
  filename    String
  size        Int
  title       String?
  description String?
  createdAt   DateTime @default(now())
}
